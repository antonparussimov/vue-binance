    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "unminified-webpack-plugin": "^2.0.0",
    "vue": "^2.5.17",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.3.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
