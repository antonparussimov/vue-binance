  "bugs": {
    "url": "https://github.com/ankurk91/vue-loading-overlay/issues"
  },
  "homepage": "https://github.com/ankurk91/vue-loading-overlay",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch --onlyChanged --notify",
